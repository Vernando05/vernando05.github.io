{
  "name": "vs-minimal",
  "type": "module",
  "version": "0.0.1",
  "author": "Vernando Simbolon (https://github.com/Vernando05)",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "clean": "rimraf .next out coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check-types": "tsc --noEmit --pretty",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "commit": "cz",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "http-server storybook-static --port 6006 --silent",
    "serve-storybook": "run-s storybook:*",
    "test-storybook:ci": "start-server-and-test serve-storybook http://127.0.0.1:6006 test-storybook",
    "prepare": "husky",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@astrojs/react": "^4.2.0",
    "@astrojs/sitemap": "^3.2.1",
    "@date-fns/tz": "^1.2.0",
    "@gsap/react": "^2.1.2",
    "@hookform/resolvers": "^3.10.0",
    "@react-three/fiber": "^9.0.0-rc.1",
    "@sentry/astro": "^8.53.0",
    "@spotlightjs/astro": "^2.4.3",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "alea": "^1.0.1",
    "astro": "^5.2.2",
    "date-fns": "^4.1.0",
    "gsap": "^3.12.7",
    "immer": "^10.1.1",
    "lenis": "^1.1.20",
    "lottie-react": "^2.4.1",
    "ogl": "^1.0.11",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-typed": "^2.0.12",
    "react-use": "^17.6.0",
    "simplex-noise": "^4.0.3",
    "tailwindcss": "^3.4.17",
    "three": "^0.172.0",
    "three-stdlib": "^2.35.13",
    "use-immer": "^0.11.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.1.0",
    "@astrojs/tailwind": "^5.1.5",
    "@chromatic-com/storybook": "^3.2.4",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@commitlint/cz-commitlint": "^19.6.1",
    "@eslint-react/eslint-plugin": "^1.26.0",
    "@playwright/test": "^1.50.0",
    "@storybook/addon-essentials": "^8.5.2",
    "@storybook/addon-interactions": "^8.5.2",
    "@storybook/addon-onboarding": "^8.5.2",
    "@storybook/blocks": "^8.5.1",
    "@storybook/react": "^8.5.1",
    "@storybook/react-vite": "^8.5.2",
    "@storybook/test": "^8.5.1",
    "@tailwindcss/vite": "^4.0.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "@types/three": "^0.172.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.1",
    "cssnano": "^7.0.6",
    "eslint": "^9.19.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-storybook": "^0.11.2",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "globals": "^15.14.0",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "inquirer": "^9.3.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.4.3",
    "npm-run-all": "^4.1.5",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "postcss": "^8.5.1",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "semantic-release": "^24.2.1",
    "start-server-and-test": "^2.0.10",
    "storybook": "^8.5.2",
    "tailwindcss-motion": "^1.0.1",
    "type-fest": "^4.33.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.4",
    "vitest-fail-on-console": "^0.7.1"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github"
    ]
  }
}
